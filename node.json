{
    "name": "Download Model",
    "category": "advanced/loaders",
    "inputs": [
      {
        "name": "LINK",
        "type": "string"
      },
      {
        "name": "OUTPUT",
        "type": "string"
      },
      {
        "name": "MODEL_TYPE",
        "type": "enum",
        "options": ["checkpoint", "lora"]
      }
    ],
    "outputs": [
      {
        "name": "MODEL",
        "type": "model"
      },
      {
        "name": "CLIP",
        "type": "clip",
        "optional": True
      },
      {
        "name": "VAE",
        "type": "vae",
        "optional": True
      }
    ],
    "function": "download_model"
  }