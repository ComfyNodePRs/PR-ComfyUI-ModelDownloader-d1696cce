{
  "name": "Model Downloader",
  "category": "loaders",  // Adjust the category if needed
  "class": "ModelDownloader",
  "function": "load_checkpoint",
  "inputs": [
    {
      "name": "ckpt_name_or_link",
      "options": [
        [folder_paths.get_filename_list("checkpoints"), "Local Checkpoint"],
        "STRING"
      ]
    }
  ],
  "outputs": [
    {
      "name": "MODEL",
      "type": "MODEL"
    },
    {
      "name": "CLIP",
      "type": "CLIP"
    },
    {
      "name": "VAE",
      "type": "VAE"
    }
  ]
}
