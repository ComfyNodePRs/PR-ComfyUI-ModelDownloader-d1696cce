{
  "name": "Model Downloader",
  "description": "Downloads a checkpoint model from a provided URL.",
  "category": "Utilities",
  "inputs": [
    {
      "name": "LINK",
      "type": "string",
      "label": "Model Download Link"
    },
    {
      "name": "OUTPUT",
      "type": "string",
      "label": "Output Directory"
    }
  ],
  "outputs": [
    {
      "name": "MODEL",
      "type": "any",
      "label": "Loaded Model"
    }
  ]
}